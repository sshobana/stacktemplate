{
    "Description": "API Gateway with Lambda as backend",
    "Parameters": {
        "LambdaFileName": {
            "Default": "$ stacktemplate/index.zip",
            "Type": "String"
        },
        "Role":{
            "Default":"$ stacktemplate/role.json",
            "Type":"String"
        }
       

    },
    "Resources": {
      
        "Lambda": {
            "Properties": {
                "Code": {
                    
                    "Key": {
                        "Ref": "LambdaFileName"
                    }
                },
                "Handler": "index.lambda_handler",
                "FunctionName":"Upload",
                "MemorySize": 128,
                "Role": {
                    "Rolekey":{
                    "Ref":"Role"
                },
                "Runtime": "python2.7",
                "Timeout": 30
            },
            "Type": "AWS::Lambda::Function"
        }
    },


}



